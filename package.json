{
  "name": "esc-project",
  "version": "1.0.0",
  "description": "Welcome to the backend :) How do you run our program? 1. go to the backendPrinceton branch 2. type 'npm install' 3. type 'npm start' 4. go to the file 'blueprint' and insert that json file into postman 5. test the apis on postman :)",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "install": "node-gyp rebuild",
    "start-dev": "nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrincetonPoh/esc-project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PrincetonPoh/esc-project/issues"
  },
  "homepage": "https://github.com/PrincetonPoh/esc-project#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.18",
    "nodemon": "^2.0.7",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.2"
  },
  "gypfile": true,
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  }
}
